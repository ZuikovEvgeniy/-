    "f = open('input.txt', 'r')\n",
    "text = f.read().split()\n",
    "a = text[-1]\n",
    "b = text[0: -1]\n",
    "c = a.join(b)\n",
    "d = eval(c)\n",
    "g = open('output.txt', 'w')\n",
    "g.write(str(d))\n",
    "f.close()\n",
    "g.close()"
